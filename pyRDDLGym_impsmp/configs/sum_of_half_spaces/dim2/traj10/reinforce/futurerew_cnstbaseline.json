{ "useGPU": false,
  "use64bit": true,
  "debug_nans": true,
  "enable_jit": true,
  "seed": 3264,
  "state_dim": 2,
  "action_dim": 2,
  "n_iters": 50,
  "models": {
    "type": "sum_of_half_spaces",
    "params": {
      "n_summands": 10,
      "instance_idx": 0,
      "reward_shift": 0.0,
      "specs": {
        "train_model": {
          "is_relaxed": false,
          "initial_state_config": {
            "type": "normal",
            "params": {
              "mean": 0.0,
              "scale": 2.0
            }
          },
          "compiler_kwargs": {}
        },
        "eval_model": {
          "is_relaxed": false,
          "initial_state_config": {
            "type": "normal",
            "params": {
              "mean": 0.0,
              "scale": 2.0
            }
          },
          "compiler_kwargs": {}
        }
      }
    }
  },
  "bijector": {
    "type": "identity",
    "params": {}
  },
  "policy": {
    "type": "multivar_normal_with_linear_parametrization",
    "params": {
        "compute_jacobians_analytically": true,
        "cov_lower_cap": 0.05
    }
  },
  "adv_estimator": {
    "type": "future_traj_reward_w_constant_baseline",
    "params": {
      "gamma": 0.9,
      "base_val": -10.0
    }
  },
  "optimizer": {
    "type": "adabelief",
    "params": {
      "learning_rate": 0.1
    }
  },
  "algorithm": {
    "type": "reinforce",
    "params": {
      "batch_size": 32,
      "eval_batch_size": 32,
      "epsilon": 1e-12,
      "verbose": true
    }
  },
  "save_to": null
}
