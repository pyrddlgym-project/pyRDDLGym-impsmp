{ "useGPU": false,
  "use64bit": true,
  "debug_nans": true,
  "enable_jit": true,
  "seed": 3264,
  "state_dim": 5,
  "action_dim": 1,
  "n_iters": 500,
  "checkpoint_freq": 100,
  "models": {
    "type": "rddl_cartpole_balance",
    "params": {
      "reward_shift": 0.0,
      "dense_reward": true,
      "solver": "adams_bashforth_5step",
      "specs": {
        "train_model": {
          "is_relaxed": false,
          "initial_state_config": {
            "type": "dm_control",
            "params": {}
          },
          "compiler_kwargs": {}
        }
      }
    }
  },
  "bijector": {
    "type": "identity",
    "params": {}
  },
  "policy": {
    "type": "multivar_normal_with_mlp_parametrization",
    "params": {
        "cov_lower_cap": 0.05
    }
  },
  "adv_estimator": {
    "type": "future_traj_reward",
    "params": {
      "gamma": 0.95
    }
  },
  "optimizer": {
    "type": "adabelief",
    "params": {
      "learning_rate": 0.01
    }
  },
  "algorithm": {
    "type": "reinforce",
    "params": {
      "batch_size": 256,
      "eval_batch_size": 32,
      "epsilon": 1e-12,
      "verbose": true
    }
  },
  "save_to": "/tmp"
}
